<p>
	<% unless user_signed_in? %>
		<%= link_to 'Ingresar', login_path %>
		<%= link_to 'Registrar', sign_up_path %>
	<% else %>
		<%= link_to current_user.name, profile_path %> <small><%= current_user.role %></small>
		<%= link_to 'Salir', logout_path, method: :delete, data: { confirm: "Seguro que desea salir?" } %>
	<% end %>
</p>